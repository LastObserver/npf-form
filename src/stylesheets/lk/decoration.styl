$decoration = '.ngx-decoration'

{ $decoration }
  &_theme_nye
    position relative

    .npf-header
      background-position center
      background-repeat no-repeat
      background-image url('data:image/png;base64,')

    .ngx-input-container
      &__control
        &_theme_nye
          position relative

          &::before
            display block
            position absolute
            content ''
            width 46px
            height 35px
            top -22px
            right -15px
            background url('data:image/png;base64,') no-repeat center

    .ngx-toolbar
      &::before
      &::after
        position absolute
        content ''
        background-position center
        background-repeat no-repeat
        background-size cover
        z-index -1
      
      &::before
        width 60px
        height 60px
        right 480px
        top -32px
        background-image url('data:image/svg+xml;base64,')

      &::after
        width 55px
        height 55px
        right 308px
        top 22px
        background-image url('data:image/svg+xml;base64,')

    .ngx-footer
      position relative

      .ngx-section
        position relative
        z-index 10

      &__container
        &::before
        &::after
          display block
          position absolute
          content ''
          background-position center
          background-repeat no-repeat
          background-size cover
          z-index 1

        &::before
          width 213px
          height 213px
          left 46%
          top 42%
          background-image url('data:image/svg+xml;base64,')

          +below('sm')
            left 25%
            top 32%
        
        &::after
          width 116px
          height 116px
          right 22%
          bottom 72px
          background-image url('data:image/svg+xml;base64,')

        hr
          height 8px
          border none
          background url('data:image/png;base64,') repeat-x left center
